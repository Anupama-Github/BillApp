<%= form_for(@item) do |f| %>
f.autocomplete_field :item_name, autocomplete_items_name_items_path
  <% if @item.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@item.errors.count, "error") %> prohibited this item from being saved:</h2>

      <ul>
      <% @item.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :name %><br />
  <!-- <%= f.text_field :name %>
    <%= text_field_with_auto_complete :item, name %>-->
     f.autocomplete_field :item_name, autocomplete_item_name_items_path
  </div>
 

  <div class="field">
    <%= f.label :item_id %><br />
    <%= f.number_field :item_id %>
  </div>
  <div class="field">
    <%= f.label :length %><br />
    <%= f.number_field :length %>
  </div>
  <div class="field">
    <%= f.label :breadth %><br />
    <%= f.number_field :breadth %>
  </div>
  <div class="field">
    <%= f.label :custom_id %><br />
    <%= f.number_field :custom_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
